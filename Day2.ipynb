   ],
   "source": [
    "prev, curr = 0,1\n",
    "total_sum = 0\n",
    "while True:\n",
    "    prev,curr = curr,prev + curr\n",
    "    if curr >= 4000000:\n",
    "        break\n",
    "    if curr%2 == 0:\n",
    "        total_sum += curr\n",
    "print(total_sum)"
   ]
